<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Version>1.0.0</Version>
        <Authors>Samuel Lucas</Authors>
        <Description>A .NET implementation of XAES-256-GCM, an extended nonce version of AES-GCM.</Description>
        <Copyright>Copyright (c) 2024 Samuel Lucas</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/samuel-lucas6/XAES-256-GCM-dotnet</PackageProjectUrl>
        <RepositoryUrl>https://github.com/samuel-lucas6/XAES-256-GCM-dotnet</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>aes-gcm aes-256 aes aes-cmac cmac extended-nonce</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

</Project>
